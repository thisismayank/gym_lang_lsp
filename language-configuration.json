{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [["\"", "\""]],
  "autoClosingPairs": [["\"", "\""]],
  "surroundingPairs": [["\"", "\""]],
  "indentationRules": {
    "increaseIndentPattern": "",
    "decreaseIndentPattern": ""
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "folding": {
    "markers": {
      "start": "^\\s*#\\s*region\\b",
      "end": "^\\s*#\\s*endregion\\b"
    }
  }
}
